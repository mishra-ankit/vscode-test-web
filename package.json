{
  "name": "vscode-web",
  "version": "0.0.24",
  "scripts": {
    "install-extensions": "yarn --cwd=fs-provider && yarn --cwd=sample",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "prepublishOnly": "yarn compile",
    "test": "eslint src --ext ts && tsc --noEmit",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "compile-fs-provider": "yarn --cwd=fs-provider compile-web",
    "compile-sample": "yarn --cwd=sample compile-web",
    "sample": "npm run compile && npm run compile-sample && node . --extensionDevelopmentPath=sample sample/test-workspace",
    "build": "npm run compile && node . && ncp workbench-default.html vscode-web-stable/index.html",
    "serve": "serve vscode-web-stable --cors -p 1234 --single"
  },
  "main": "./out/index.js",
  "bin": {
    "vscode-test-web": "./out/index.js"
  },
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.0",
    "minimist": "^1.2.6",
    "serve": "^13.0.2",
    "vscode-uri": "^3.0.3"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^12.19.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-plugin-header": "^3.1.1",
    "ncp": "^2.0.0",
    "typescript": "^4.5.5"
  }
}
